<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee/WEB-INF/schema/web-facesconfig_2_0.xsd"
              version="2.0" metadata-complete="false">
  
    <!-- Managed Beans -->
    <managed-bean>
        <managed-bean-name>doctorDao</managed-bean-name>
        <managed-bean-class>com.infinite.jsf.admin.daoImpl.DoctorDaoImpl</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>  <!-- Changed to session scope -->
    </managed-bean>

    <managed-bean>
        <managed-bean-name>doctor</managed-bean-name>
        <managed-bean-class>com.infinite.jsf.provider.model.Doctors</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>  <!-- Changed to session scope -->
    </managed-bean>

    <managed-bean>
        <managed-bean-name>provider</managed-bean-name>
        <managed-bean-class>com.infinite.jsf.provider.model.Provider</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>  <!-- Changed to session scope -->
    </managed-bean>
      <managed-bean>
        <managed-bean-name>doctorController</managed-bean-name>
        <managed-bean-class>com.infinite.jsf.admin.controller.DoctorController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>  <!-- doctorController stays session -->
        <managed-property>
            <property-name>doctorDao</property-name>
            <property-class>com.infinite.jsf.admin.daoImpl.DoctorDaoImpl</property-class>
            <value>#{doctorDao}</value>
        </managed-property>
        
         <managed-property>
            <property-name>provider</property-name>
            <property-class>com.infinite.jsf.provider.model.Provider</property-class>
            <value>#{provider}</value>
        </managed-property>
        
        <managed-property>
            <property-name>doctor</property-name>
            <property-class>com.infinite.jsf.provider.model.Doctors</property-class>
            <value>#{doctor}</value>
        </managed-property>
       
    </managed-bean>

</faces-config>
